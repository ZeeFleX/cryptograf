{
  "name": "rest-api-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development node_modules/nodemon/bin/nodemon.js index.js",
    "docker": "node index.js",
    "update": "git pull && npm install",
    "migrate": "MIGRATIONS_SEEDS=true node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "MIGRATIONS_SEEDS=true node_modules/.bin/sequelize db:migrate:undo",
    "migrate:undo:all": "MIGRATIONS_SEEDS=true node_modules/.bin/sequelize db:migrate:undo:all",
    "seed": "MIGRATIONS_SEEDS=true node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "MIGRATIONS_SEEDS=true node_modules/.bin/sequelize db:seed:all",
    "seed:undo:all": "MIGRATIONS_SEEDS=true node_modules/.bin/sequelize db:seed:undo:all",
    "test": "node_modules/mocha/bin/mocha --timeout 10000 ./tests/**/*.js --exit",
    "test:rebase": "npm run migrate:undo:all && npm run migrate && npm run seed && node_modules/mocha/bin/mocha --timeout 10000 ./tests/**/*.js --exit"
  },
  "author": "ik.code",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt-nodejs": "0.0.3",
    "cluster": "^0.7.7",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "express": "~4.16.3",
    "express-validator": "^5.3.1",
    "fabric-ca-client": "~1.4.4",
    "fabric-client": "~1.4.4",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.21.0",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "node-binance-api": "^0.10.0",
    "pluralize": "^8.0.0",
    "sequelize": "^5.21.3",
    "ssh2-promise": "^0.1.5",
    "swagger-express-middleware": "^2.0.2",
    "swagger-ui-express": "^4.0.7",
    "validate": "^5.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  }
}
